ult\t:= fbTcpServer.Result;\t// connection failed, check the result\n\t\tIF (etTcpServerResult = TCPUDP.ET_Result.ClientListTooSmall) THEN\n\t\t\txWarnMaxNbOfClients\t:= TRUE;\n\t\t\t// Non critical: reset the result, so that the  TCP Server is not blocked\n\t\t\tfbTcpServer.ResetResult();\n\t\tELSE\n\t\t\tsStepFailed\t\t:= 'Accept: ';\n\t\t\txAlarmAccept\t:= TRUE;\n\t\tEND_IF\n\tEND_IF\nEND_IF\t","LineInfoPersistence":"(string)8502bd50-ebe9-49b0-b98a-2ed379979754_Impl_LineIds"}}}},"UniqueIdGenerator":"(string)30"}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","8ac092e5":"{8ac092e5-3128-4e26-9e7e-11016c6684f2}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}