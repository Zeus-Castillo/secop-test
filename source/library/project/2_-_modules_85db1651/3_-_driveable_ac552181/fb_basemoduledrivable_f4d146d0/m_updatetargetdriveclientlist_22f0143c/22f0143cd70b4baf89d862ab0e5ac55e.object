n\n// Register client as the last one that sent the target change request\n// (will be the only one receiving the \"changed\" message once completed. Rest of clients will receive \"update\" messages)\niq_stTargetWrite.stTargetChangeClient.sIp := i_stClient.sIp;\niq_stTargetWrite.stTargetChangeClient.uiPort := i_stClient.uiPort;\n","LineInfoPersistence":"(string)22f0143c-d70b-4baf-89d8-62ab0e5ac55e_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)METHOD M_UpdateTargetDriveClientList : BOOL\nVAR_INPUT\n\ti_stClient: ST_Subscriber;\nEND_VAR\n","LineInfoPersistence":"(string)22f0143c-d70b-4baf-89d8-62ab0e5ac55e_Decl_LineIds"}}}}}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","f8a58466":"{f8a58466-d7f6-439f-bbb8-d4600e41d099}","string":"System.String"}}},"FormatVersion":"1.0"}