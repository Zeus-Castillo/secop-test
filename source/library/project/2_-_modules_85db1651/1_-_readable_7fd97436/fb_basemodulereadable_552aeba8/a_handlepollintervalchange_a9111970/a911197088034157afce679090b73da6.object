rrorLatched:= FALSE);\t\t\n\nELSIF StrIsNullOrEmptyA(pstData:= ADR(i_sData)) THEN // No data to parse\n\t// Return \"BadJSON\" error\n\tA_ReturnErrorDataNotParsed();\n\nELSIF NOT M_CheckIfDataIsNumeric(i_sDataToParse:= i_sData, i_xMustBeInteger:= FALSE) THEN // Unexpected data type\n\t// Return \"WrongType\" error\n\tA_ReturnErrorWrongType();\n\t\nELSE // Process new value for pollinterval\n\tlrPollintervalNewVal := STRING_TO_LREAL(i_sData);\n\tIF lrPollintervalNewVal > iq_stPollInterval.lrMax OR lrPollintervalNewVal < iq_stPollInterval.lrMin THEN // Value out of range\n\t\t\n\t\t// Return \"RangeError\" error\n\t\tiq_stErrorReport.sClass := 'RangeError';\n\t\tiq_stErrorReport.sDescription := 'Value must be between ';\n\t\tsPollIntervalMin := LREAL_TO_STRING(iq_stPollInterval.lrMin);\n\t\tStrConcatA(pstFrom:= ADR(sPollIntervalMin), pstTo:= ADR(iq_stErrorReport.sDescription), iBufferSize:= UINT_TO_INT(GPL.Gc_uiMaxSizeDescription));\t\n\t\tStrConcatA(pstFrom:= ADR(' and '), pstTo:= ADR(iq_stErrorReport.sDescription), iBufferSize:= UINT_TO_INT(GPL.Gc_uiMaxSizeDescription));\t\n\t\tsPollIntervalMax := LREAL_TO_STRING(iq_stPollInterval.lrMax);\n\t\tStrConcatA(pstFrom:= ADR(sPollIntervalMax), pstTo:= ADR(iq_stErrorReport.sDescription), iBufferSize:= UINT_TO_INT(GPL.Gc_uiMaxSizeDescription));\t\n\t\tM_ReplyWithErrorStraightAway(i_xErrorLatched:= FALSE);\t\t\n\t\t\n\tELSE\t\n\t\t// Apply requested value\n\t\tiq_stPollInterval.lrValue := LIMIT(iq_stPollInterval.lrMin,lrPollintervalNewVal,iq_stPollInterval.lrMax);\n\t\tM_AddDataReportToReplyMessage(i_xReturnError := FALSE,  i_sAction := 'changed', i_sAccessible := i_sAccessible, i_sDataReport:= LREAL_TO_STRING(iq_stPollInterval.lrValue));\n\t\txPollIntervalChanged := TRUE;\n\t\t// Register client that sent the pollinterval change request\n\t\tiq_stPollInterval.stPollintervalChangeClient.sIp := i_stClientMonitored.sIp;\n\t\tiq_stPollInterval.stPollintervalChangeClient.uiPort := i_stClientMonitored.uiPort;\n\tEND_IF\nEND_IF\n","LineInfoPersistence":"(string)a9111970-8803-4157-afce-679090b73da6_Impl_LineIds"}}}},"UniqueIdGenerator":"(string)89"}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","8ac092e5":"{8ac092e5-3128-4e26-9e7e-11016c6684f2}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}