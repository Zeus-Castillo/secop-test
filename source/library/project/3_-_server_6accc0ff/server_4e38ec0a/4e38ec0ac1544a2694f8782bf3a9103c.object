{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)4e38ec0a-c154-4a26-94f8-782bf3a9103c","ParentGuid":"(Guid)00000000-0000-0000-0000-000000000000","Name":"(string)Server","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)24568a24-c491-472c-a21f-ee5d33859fab","@Value":{"@Type":"24568a24","@Value":{"MemoryReserveForOnlineChange":"(int)0","ExcludeFromBuild":"(bool)False","External":"(bool)False","EnableSystemCall":"(bool)False","CompilerDefines":"(string)","LinkAlways":"(bool)False","Undefines":{"@Type":"[string]","@Value":[]}}}},{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)21af5390-2942-461a-bf89-951aaf6999f1","@Value":"(Guid)6accc0ff-d348-40ac-8938-2bc8efd82d9d"}]}}}}]},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"24568a24":"{24568a24-c491-472c-a21f-ee5d33859fab}","2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","bool":"System.Boolean","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","int":"System.Int32","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)\n// TCP server reset clock. If an exchange error occured, check 'q_sLastDiagMessage' for information on the last registered error \nfbBlink2s(ENABLE:= TRUE, TIMELOW:= T#1S, TIMEHIGH:= T#1S);\nfbRtrig2s(CLK:= fbBlink2s.OUT);\nxOnEvery2s := fbRtrig2s.Q;\nIF xOnEvery2s THEN x2sSinceStartup:= TRUE; END_IF\nxCmdClearResult := xError AND xOnEvery2s;\n\n// Dispatcher. Prepare messages to be sent to clients\nFOR uiClientId := 1 TO TCPUDP.GPL.Gc_uiTCPServerMaxConnections DO\n\t\n\t// Monitor disconnected clients\n\t{IF project_defined(SCHNEIDER_ELECTRIC)} \n\tfbDisconnectedClientIp[uiClientId](i_sVar:= astClientInfo[uiClientId].sIp);\n\tfbDisconnectedClientPort[uiClientId](i_rVar:= UINT_TO_REAL(astClientInfo[uiClientId].uiPort));\n\txClientDisconnectedFlag := \n\t\tfbDisconnectedClientIp[uiClientId].q_xHasChanged AND fbDisconnectedClientIp[uiClientId].i_sVar ='' AND fbDisconnectedClientIp[uiClientId].q_sPreviousValue <> ''\n\t\tAND fbDisconnectedClientPort[uiClientId].q_xHasChanged \tAND fbDisconnectedClientPort[uiClientId].i_rVar <=0 AND fbDisconnectedClientPort[uiClientId].q_rPreviousValue > 0;\n\tIF xClientDisconnectedFlag THEN\n\t\tsDisconnectedCientIp :=  fbDisconnectedClientIp[uiClientId].q_sPreviousValue;\n\t\tuiDisconnectedCientPort :=  REAL_TO_UINT(fbDisconnectedClientPort[uiClientId].q_rPreviousValue);\n\t\tastClientInfo[uiClientId].stDataToSend.sData:='';\n\tEND_IF\n\t{END_IF}\n\t\n\t// Monitor new data from clients\n\t{IF project_defined(SCHNEIDER_ELECTRIC)} \n\tfbReceivedData[uiClientId](i_rVar:= ULINT_TO_REAL(astClientInfo[uiClientId].uliTotalBytesReceived));\t\n\tIF xMessageFragNeeded THEN // {IF defined(variable:SEC.PLC_GVL.ETH_R.i_byIPAddress_If2)} // M251\n\t\t// Clear message buffer if the client is no longer conencted\n\t\tIF asMessageToSend[uiClientId] <> '' AND astClientInfo[uiClientId].uiPort<=0 THEN\n\t\t\tasMessageToSend[uiClientId]:='';\n\t\tEND_IF\n\t\t// Send chunks from the message buffer that weren't sent earlier due to the TCP buffer's limit, and update message buffer\n\t\tIF asMessageToSend[uiClientId] <> '' AND NOT astClientInfo[uiClientId].axPushData THEN \n\t\t\tA_MessageFragmentation();\n\t\tEND_IF // {END_IF}\n\tEND_IF\n\t{END_IF}\t\n\t\t\n\t// Raise synchronous mode flag if there is a new client request to process\n\t{IF project_defined(SCHNEIDER_ELECTRIC)} \n\txSyncModeRequest := fbReceivedData[uiClientId].q_xHasChanged\n\t\t\t\t\t\tAND astClientInfo[uiClientId].stDataReceived.sData <> sClientDataToIgnore\n\t\t\t\t\t\tAND astClientInfo[uiClientId].uiPort>0;\n\t{END_IF}\n\t\n\t// Parse client message in synchronous mode. Retrieve action, specifier (module & accessible) and data while looking for errors\n\t{IF project_defined(SCHNEIDER_ELECTRIC)} \t\n\tIF xSyncModeRequest THEN\n\t\tsMessageToParse:=astClientInfo[uiClientId].stDataReceived.sData;\n\t\tA_ParseClientMessage();\n\tEND_IF\n\t{END_IF}\t\n\t\n\t// Process client requests both in synchronous and asynchronous mode\n\t{IF project_defined(SCHNEIDER_ELECTRIC)} \n\tstClientMonitored.sIp := astClientInfo[Server.uiClientId].sIp;\n\tstClientMonitored.uiPort := astClientInfo[Server.uiClientId].uiPort;\n\tstClientDisconnected.sIp := sDisconnectedCientIp;\n\tstClientDisconnected.uiPort := uiDisconnectedCientPort;\n\txFirstSecopClient := uiClientId = 1;\n\txAllSecopClientsDone := uiClientId = GPL.Gc_uiMaxNoOfClients;\n\t{END_IF} \t\t\n\tA_ProcessClientRequest();\n\t\n\t// Send message to client if there is data to send (push)\n\t{IF project_defined(SCHNEIDER_ELECTRIC)} \n\tIF xMessageFragNeeded THEN //{IF defined(variable:SEC.PLC_GVL.ETH_R.i_byIPAddress_If2)} // M251\n\t\tStu.StrConcatA(ADR(sReplyMessage), ADR(asMessageToSend[uiClientId]), UINT_TO_INT(SIZEOF(asMessageToSend[uiClientId])));\n\t\tIF asMessageToSend[uiClientId] <> '' THEN\n\t\t\tA_MessageFragmentation();\n\t\tEND_IF\n\tELSE // {ELSE} // M262\n\t\tastClientInfo[uiClientId].stDataToSend.sData := sReplyMessage;\n\t\tIF astClientInfo[uiClientId].stDataToSend.sData <> '' THEN\n\t\t\tastClientInfo[uiClientId].axPushData := TRUE;\n\t\tEND_IF\n\tEND_IF // {END_IF}\n\t{END_IF}\n\nEND_FOR;\n\t\n// TCP Server accepting sockets from multiple clients\n{IF project_defined(SCHNEIDER_ELECTRIC)} \nfbTcpServer(\n\ti_xEnable := x2sSinceStartup,\n\ti_xEthIfReady := GVL.G_st_SecNode.xTcpServerInterfaceReady,\n\ti_xCmdListen:= x2sSinceStartup AND GVL.G_st_SecNode.xTcpServerInterfaceReady,\n\ti_xCmdClose:= xCmdClose,\n\ti_xCmdDisconnectAll:= xCmdDisconnectAll, \n\ti_xCmdClearResult:= xCmdClearResult, \n\ti_sIpAddrServer:= GVL.G_st_SecNode.sTcpServerIp,\n\ti_uiTcpServerPort:= GVL.G_st_SecNode.uiTcpServerPort,\n\ti_timTimeoutInactivity:= T#30M,\n\ti_xTlsSelected:= FALSE,\n\ti_etCertVerifyMode:= TCPUDP.ET_CertVerifyMode.NotVerified,\n\tiq_astClientInfo := astClientInfo,\n  \t//q_astDataReceived := ,\t\n\t//q_astConnectedClients=> , \n\tq_uiNbOfConnectedClients=> uiNbOfConnectedClients,\n\tq_xPortOpen=> xPortOpen,\n\tq_sDiagMessage=> sDiagMessage,\n\tq_xDiag=> xError,\n\tq_sTcpServerState=> sTcpServerState,\n\tq_sTcpServerResult=> sTcpServerResult,\n\tq_sLastErrorDiagMessage=> sLastErrorDiagMessage);\t\n\n(*\t\n{ELSIF project_defined(WAGO)}\n\t...\n{ELSIF project_defined(ABB)}\n\t...\n...\n*)\n{ELSE}\n\t{error 'No platform defined! Please include it in the list of global defines in the Compile oprtions of the project settings}\n{END_IF}\n","LineInfoPersistence":"(string)4e38ec0a-c154-4a26-94f8-782bf3a9103c_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)PROGRAM Server\nVAR\n{IF project_defined(SCHNEIDER_ELECTRIC)} \n\t// SCHNEIDER_ELECTRIC Tcp Server handling\n\tfbTcpServer: FB_ASCIIoverTCPserver_SE;\t\n\tfbReceivedData: ARRAY[1..TCPUDP.GPL.Gc_uiTCPServerMaxConnections] OF FB_MonitorNumValue;\n\tfbDisconnectedClientIp: ARRAY[1..TCPUDP.GPL.Gc_uiTCPServerMaxConnections] OF FB_MonitorSmallString;\n\tfbDisconnectedClientPort: ARRAY[1..TCPUDP.GPL.Gc_uiTCPServerMaxConnections] OF FB_MonitorNumValue;\n\tastClientInfo: ARRAY [1..TCPUDP.GPL.Gc_uiTCPServerMaxConnections] OF ST_ClientInfo_SE;\n\tasMessageToSend: ARRAY[1..TCPUDP.GPL.Gc_uiTCPServerMaxConnections] OF STRING(GPL_TcpServer_SE.Gc_uiSendingMessageMaxSize);\n\tsClientDataToIgnore: STRING(GPL.Gc_uiMaxSizeMessage) := 'RN';\n\txCmdClose: BOOL;\n\txCmdDisconnectAll: BOOL;\t\n\tsTcpServerState: STRING;\n\tsTcpServerResult: STRING;\n\tsLastErrorDiagMessage: STRING;\n\tuiNbOfConnectedClients: UINT;\n\txPortOpen: BOOL;\n\tsDiagMessage: STRING;\n\t/// Auxiliary variables\n\txMessageFragNeeded : BOOL := GPL_TcpServer_SE.Gc_uiDataTransferChunkSize < GPL_TcpServer_SE.Gc_uiSendingMessageMaxSize; // Message fragmentation needed\t\n\tiStartPos: INT;\n\tiEndPos: INT;\n\tsDisconnectedCientIp: STRING;\n\tuiDisconnectedCientPort: UINT;\n{END_IF}\n\n\t// Tcp Server handling\n\t// -----------------------\n\txCmdClearResult: BOOL;\n\tuiClientId: UINT;\n\tfbBlink2s : BLINK;\n\tfbRtrig2s : R_TRIG;\t\n\txOnEvery2s : BOOL;\n\tx2sSinceStartup : BOOL := FALSE;\n\txError: BOOL;\n\txClientDisconnectedFlag: BOOL;\n\txSyncModeRequest: BOOL;\t\n\tstClientMonitored : ST_Subscriber;\n\tstClientDisconnected : ST_Subscriber;\n\txFirstSecopClient: BOOL;\n\txAllSecopClientsDone: BOOL;\n\t\n\t// Client request handling\n\t// -----------------------\n\t// Message to parse\n\tsMessageToParse: STRING(GPL.Gc_uiMaxSizeMessage);\t\n\t// Action\n\tsAction: STRING;\n\t// Specifier. Usually contains a module name and an accessible (parameter, property or command)\n\tsSpecifier: STRING(((2 * GCL.Gc_uiMaxSizeIdentifier) + 1));\n\t// Data\n\tsData: STRING(GPL.Gc_uiMaxSizeMessage);\n\t// Module\n\tsModuleName: STRING(GCL.Gc_uiMaxSizeIdentifier);\n\t// Accessible (parameter, a property or a command)\n\tsAccessibleName: STRING(GCL.Gc_uiMaxSizeIdentifier);\n\t/// Reply message\n\tsReplyMessage: STRING(GPL.Gc_uiMaxSizeMessage);\n\t/// Processs SEC node and module-wise requests\n\tfbSecNode: FB_SecNode;\t\n\t/// Auxiliary variables\n\tiIndexSubMessage: INT;\n\tiCountSubMessages: INT;\n\txActionIsImplemented: BOOL;\n\txSpecifierFormatOk: BOOL;\t\n\tiIndexAction: INT;\n\tetDelimiter: ET_Delimiter;\n\tetTerminator: ET_Terminator;\n\tiPos: INT;\n\tsTemp: STRING(GPL.Gc_uiMaxSizeMessage);\t\nEND_VAR\n","LineInfoPersistence":"(string)4e38ec0a-c154-4a26-94f8-782bf3a9103c_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)1722","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}